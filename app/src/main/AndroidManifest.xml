<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.eduhub.company">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CALL_PHONE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/launcher_eduhub"
        android:label="@string/app_name"
        android:roundIcon="@drawable/launcher_eduhub"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".activities.DownloadActivity"></activity>
        <activity android:name=".activities.AddStudent" />
        <activity android:name=".activities.Comments" />
        <activity
            android:name=".activities.ReportIssue" />
        <activity
            android:name=".activities.ChatRoom"
            android:label="@string/title_activity_chat_room"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activities.teacher.SyllabusT" />
        <activity
            android:name=".activities.teacher.TimeTableT" />
        <activity
            android:name=".activities.teacher.ProjectsT" />
        <activity
            android:name=".activities.teacher.MarksT" />
        <activity
            android:name=".activities.teacher.AssignmentT" />
        <activity android:name=".activities.teacher.AttendanceT" />
        <activity android:name=".activities.teacher.MainActivityT" />
        <activity android:name=".activities.parent.MainActivityParent" />
        <activity
            android:name=".activities.InfoTab.Syllabus" />
        <activity
            android:name=".activities.InfoTab.TimeTable" />
        <activity
            android:name=".activities.InfoTab.Marks" />
        <activity android:name=".activities.MainActivityStudent" />
        <activity android:name="com.soundcloud.android.crop.CropImageActivity" />
        <activity
            android:name=".activities.InfoTab.Projects" />
        <activity
            android:name=".activities.InfoTab.Assignment"/>
        <activity
            android:name=".activities.InfoTab.Attendance" />
        <activity android:name=".activities.FirstActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name=".activities.SplashScreen"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>